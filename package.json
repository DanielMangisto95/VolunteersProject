{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "export PORT=3001 && nodemon ./bin/www",
    "client": "export PORT=3000 && cd client && npm start",
    "heroku-postbuild": "cd ../client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "all": "concurrently --kill-others-on-fail \"npm start\" \"npm run client\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "concurrently": "^5.2.0"
  }
}
